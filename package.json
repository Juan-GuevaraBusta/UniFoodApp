{
  "name": "unifoodappnuevo",
  "version": "1.0.0",
  "main": "expo-router/entry",
  "scripts": {
    "start": "expo start --no-dev",
    "android": "expo start --android",
    "ios": "expo start --ios",
    "web": "expo start --web",
    "setup": "setup-unifood.bat",
    "setup:ps": "powershell -ExecutionPolicy Bypass -File setup-unifood.ps1",
    "amplify:sandbox": "npx ampx sandbox",
    "amplify:deploy": "npx ampx sandbox deploy",
    "amplify:studio": "npx ampx studio",
    "amplify:console": "npx ampx console",
    "amplify:delete": "npx ampx sandbox delete",
    "amplify:reset": "if exist .amplify rmdir /s /q .amplify && if exist amplify_outputs.json del amplify_outputs.json && npm run amplify:sandbox"
  },
  "dependencies": {
    "@aws-amplify/react-native": "^1.1.10",
    "@react-native-async-storage/async-storage": "2.1.2",
    "@react-native-community/netinfo": "^11.4.1",
    "aws-amplify": "^6.15.2",
    "expo": "53.0.18",
    "expo-constants": "~17.1.6",
    "expo-linking": "~7.1.7",
    "expo-router": "~5.1.3",
    "expo-splash-screen": "~0.30.10",
    "expo-status-bar": "~2.2.3",
    "lucide-react-native": "^0.522.0",
    "nativewind": "^4.1.23",
    "react": "19.0.0",
    "react-dom": "19.0.0",
    "react-native": "0.79.5",
    "react-native-get-random-values": "~1.11.0",
    "react-native-reanimated": "~3.17.4",
    "react-native-safe-area-context": "5.4.0",
    "react-native-screens": "~4.11.1",
    "react-native-svg": "15.11.2",
    "react-native-swiper": "^1.6.0",
    "react-native-web": "^0.20.0"
  },
  "devDependencies": {
    "@aws-amplify/backend": "^1.16.1",
    "@aws-amplify/backend-cli": "^1.8.0",
    "@babel/core": "^7.25.2",
    "@types/react": "~19.0.10",
    "aws-cdk-lib": "^2.189.1",
    "constructs": "^10.4.2",
    "esbuild": "^0.25.5",
    "tailwindcss": "^3.4.17",
    "tsx": "^4.20.3",
    "typescript": "^5.8.3"
  },
  "private": true
}